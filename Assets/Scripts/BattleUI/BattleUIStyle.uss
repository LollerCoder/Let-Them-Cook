#ActionBox{
	flex-direction: column;
	align-self: stretch;
	align-items: flex-end;
	justify-content: flex-end;
}

#Element1{
	flex-grow: 1;
	flex-direction: row;
	justify-content: center;
	align-items: center;
	align-self: flex-start;
}

.Button1{
	width: 60%;
	font-size: 12px;
}

#Element2{
	flex-grow: 1;
	align-items: center;
	align-self: stretch; 
	justify-content: space-evenly;
}

#Return{
	font-size: 12px;
	display: none;
}

#EndText{
	display: none;
}

#SkillBox{
	flex-grow: 1; 
	display: none;
	flex-direction: column; 
	align-items: flex-start; 
	justify-content: space-between; 
	align-self: flex-start; 
	flex-wrap: nowrap;
}

.Button2{
	width: 15%;
	align-self: flex-end;
}

#Box1{
	flex-grow: 1; 
	flex-direction: column; 
	justify-content: space-evenly; 
	align-items: center;
	align-self: stretch; 
	flex-wrap: wrap-reverse;
}

#CancelAttack{
	align-self: flex-end;
}